{
  "name": "react-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "start:staging": "env-cmd -f .env.staging npm run-script start",
    "start:prod": "env-cmd -f .env.production npm run-script start",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f .env.staging npm run-script build",
    "build:prod": "env-cmd -f .env.production npm run-script build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tsc": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.5",
    "@react-google-maps/api": "^2.17.1",
    "@redux-saga/core": "^1.2.1",
    "@reduxjs/toolkit": "^1.8.6",
    "apisauce": "^2.1.6",
    "env-cmd": "^10.1.0",
    "history": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "redux-first-history": "^5.1.1",
    "redux-saga": "^1.2.1",
    "typed-redux-saga": "^1.5.0",
    "typescript": "^4.8.4",
    "use-places-autocomplete": "^4.0.0",
    "web-vitals": "^3.0.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.11.7",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-module-resolver": "^1.5.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "typescript-eslint": "^0.0.1-alpha.0"
  }
}
